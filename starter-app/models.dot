digraph models_diagram {
	graph[overlap=false, splines=true]
	"Role" [shape=Mrecord, label="{Role|name :string\l}"]
	"Tag" [shape=Mrecord, label="{Tag|name :string\l}"]
	"Tagging" [shape=Mrecord, label="{Tagging|taggable_type :string\l}"]
	"User" [shape=Mrecord, label="{User|login :string\lemail :string\lcrypted_password :string\lsalt :string\lremember_token :string\lremember_token_expires_at :string\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"Tag" -> "Tagging" [taillabel="n"]
	"Tag" -> "Tagging" [label="taggables", taillabel="n", headlabel="n", arrowtail="normal"]
	"Tag" -> "Product" [taillabel="n", headlabel="n", arrowtail="normal"]
	"User" -> "Address" [label="bill_addresses", taillabel="n"]
	"User" -> "Role" [taillabel="n", headlabel="n", arrowtail="normal"]
	"User" -> "Order" [taillabel="n"]
	"User" -> "Address" [label="ship_addresses", taillabel="n"]
}
