<h4><%= :new_image.l("New Image") %></h4>
<% form_for(:image, :url => { :controller => 'images', :action => "new" }, :html => { :multipart => true }) do |form| %>
	<table class="basic-table">
	  <tr>
	    <td><%= :filename.l("Filename") %>:</td>
	    <td><%= form.file_field :uploaded_data %></td>
	  </tr>
	</table> 
 <% end %>
 
