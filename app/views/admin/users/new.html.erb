<%= error_messages_for :user %>
<h1><%= :new_user.l("New User") %></h1>
<br/>
<% form_for(:user, :url => collection_url) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%= submit_tag :create.l("Create") %>
  </p>
<% end %>
<br/>
<%= link_to :back.l('Back'), collection_url %>
